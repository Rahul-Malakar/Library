<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head.ejs') %>
<body>
    <%- include('../partials/nav.ejs') %>
    
    <div class="memberupdateform content">
        <form action="/members/update/<%= member._id %>" id="memberupdateform" method="POST">
            <label for="username">username:</label>
            <input type="text" id="username" name="username" value='<%= member.username %>'>
            <label for="email">email:</label>
            <input type="text" id="email" name="email" value='<%= member.email %>'>
            <label for="issued">Issued:</label>
            <input type="string" id="issued" name="issued" value='<%= member.issued %>'>
            <input type="submit" name="submit" value="Update" id="updatebutton">
        </form>
    </div>
    <%- include('../partials/footer.ejs') %>
</body>
</html>